{
  "name": "stylus-ts",
  "description": "Robust, expressive, and feature-rich CSS superset",
  "version": "0.54.5-alpha.1",
  "authors": [
    "Luis Vargas <luisvargastijerino@gmail.com>",
    "TJ Holowaychuk <tj@vision-media.ca>"
  ],
  "keywords": [
    "css",
    "parser",
    "style",
    "stylesheets",
    "jade",
    "language"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/stylus/stylus"
  },
  "main": "./index.js",
  "browserify": "./lib/browserify.js",
  "engines": {
    "node": "*"
  },
  "bin": {
    "stylus": "./bin/stylus"
  },
  "scripts": {
    "postinstall": "tsc",
    "prepublish": "npm prune",
    "test": "mocha test/ test/middleware/ --require should --bail --check-leaks --reporter dot",
    "test-cov": "mocha test/ test/middleware/ --require should --bail --reporter html-cov > coverage.html"
  },
  "dependencies": {
    "@types/glob": "^5.0.30",
    "@types/minimatch": "^2.0.29",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.46",
    "@types/should": "^8.1.30",
    "@types/source-map": "^0.1.29",
    "css-parse": "1.7.x",
    "debug": "*",
    "glob": "7.0.x",
    "mkdirp": "0.5.x",
    "sax": "0.5.x",
    "source-map": "0.1.x",
    "trim-right": "^1.0.1"
  },
  "devDependencies": {
    "jscoverage": "0.3.8",
    "mocha": "*",
    "should": "8.x",
    "typescript": "^2.0.3"
  },
  "bugs": {
    "url": "https://github.com/stylus/stylus/issues"
  },
  "homepage": "https://github.com/stylus/stylus",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "license": "MIT"
}
